    <artifactId>computation-servce</artifactId>
    <packaging>jar</packaging>
    <dependencies>
        <dependency>
            <groupId>g419.corpus</groupId>
            <artifactId>io</artifactId>
            <scope>system</scope>
            <systemPath>${project.basedir}/lib/g419-corpus-2.5-SNAPSHOT.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>org.glassfish</groupId>
            <artifactId>javax.json</artifactId>
            <version>1.0.2</version>
        </dependency>
        <dependency>
            <groupId>com.jetstreamdb</groupId>
            <artifactId>jetstreamdb-embedded</artifactId>
            <version>1.0.0-beta1</version>
        </dependency>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>4.12</version>
            <scope>test</scope>
        </dependency>
    </dependencies>
    <repositories>
        <repository>
